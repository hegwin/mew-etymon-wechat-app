/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  padding: 0;
  box-sizing: border-box;
  flex-wrap: wrap;
}

.section {
  width: 100%;
}

.notice {
  font-size: 28rpx;
  color: #666666;
}

.notice .highlight {
  color: #FF8C00;
  margin-left: 4rpx;
  margin-right: 4rpx;
}